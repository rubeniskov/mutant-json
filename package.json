  "bugs": {
    "url": "https://github.com/rubeniskov/mutant-json/issues"
  },
  "homepage": "https://github.com/rubeniskov/mutant-json#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint && npm test"
    }
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "eslint": "^7.14.0",
    "http-server": "^0.12.3",
    "jsdoc-to-markdown": "^6.0.1",
    "nyc": "^15.1.0",
    "codecov": "^3.8.1"
  },
